{
    "name": "sprain/swiss-qr-bill",
    "description": "A PHP library to create Swiss QR bills",
    "type": "library",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sprain/qr-code"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/validator": "^3.4|^4.2",
        "symfony/intl": "^3.4|^4.2",
        "kmukku/php-iso11649": "^1.4",
        "endroid/qr-code": "dev-bugfix-imagecopyresampled"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": { "Sprain\\SwissQrBill\\": "src" }
    }
}
