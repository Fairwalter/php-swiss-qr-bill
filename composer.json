    "type": "library",
    "require": {
        "php": ">=7.1",
        "symfony/validator": "^3.3",
        "symfony/intl": "^3.4",
        "kmukku/php-iso11649": "^1.4",
        "byrokrat/checkdigit": "^2.0",
        "endroid/qr-code": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": { "Sprain\\SwissQrBill\\": "src" }
    }
}
